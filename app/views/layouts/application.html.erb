<!DOCTYPE html>
<html>
  <head>
    <title>Isaac10-UI Demo</title>
    <%= include_gon(:init => true) %>

    <script type="text/javascript"
      <%=" src=#{Rails.application.config_for(:merchant)['submit_url']}/api/v1/isaac10_api.js" %>>
    </script>
    <script type="text/javascript"
      <%=" src=#{Rails.application.config_for(:merchant)['submit_url']}/ui/isaac10_ui.js" %>>
    </script>

    <%= stylesheet_link_tag "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
      <div class="container">
        <%= render "layouts/flashes" %>
        <%= render "layouts/navigation" %>
      </div>
    </header>
    <main role="main">
      <%= yield %>
    </main>
  </body>
</html>
